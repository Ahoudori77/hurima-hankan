{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "http": {
      "routePrefix": "api",
      "cors": {
        "allowedOrigins": [
          "http://localhost:5174",
          "http://127.0.0.1:5174"
        ],
        "allowedHeaders": [ "*" ],
        "allowedMethods": [ "GET", "POST", "OPTIONS" ],
        "supportCredentials": true
      }
    }
  }
}
