{
  "name": "frema-monorepo",
  "private": true,
  "packageManager": "pnpm@9.7.0",
  "scripts": {
    "build": "pnpm -r --filter ./apps/** --filter ./packages/** build",
    "dev": "concurrently -n liff,admin,func -c cyan,magenta,green \"pnpm --filter @frema/liff dev\" \"pnpm --filter @frema/admin dev\" \"pnpm --filter @frema/functions dev\"",
    "lint": "pnpm -r eslint .",
    "typecheck": "pnpm -r tsc -b",
    "test": "pnpm -r test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
